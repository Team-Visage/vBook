<%= link_to new_post_path do %>
  New post
<% end %>
<% @posts.reverse_each do |post| %>
<div class="post">
  <div class="post_head">
  <img src="<%=User.find(post.user_id).image_url%>" width="8%">
  <h3><%=  User.find(post.user_id).username%> :
      <%= post.created_at %>

      <%= post.post_likes %>
  </h3>
  </div>
  <div class="post_body">
  <p><%= post.message %></p>
  </div>
</div>
<% end %>
